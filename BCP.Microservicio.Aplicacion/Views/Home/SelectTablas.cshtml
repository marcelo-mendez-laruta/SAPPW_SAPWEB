@{
    ViewData["Title"] = "Seleccione Elemento: ";
}
    <div class="container border border-primary" style="background-color:lightgrey">
        <header>
            <h4>Seleccione Elemento:</h4>
        </header>
        <select id="cbSeltab" class="form-control" size="20">
        </select>
        <footer>
            <center>
                <div class="row">
                    <button type="button" class="col btn btn-outline-secondary m-1">
                        <i class="fas fa-check icono-2x"></i><br />Aceptar
                    </button>
                    <button type="button" class="col btn btn-outline-secondary m-1">
                        <i class="fas fa-window-close icono-2x"></i><br />Cerrar
                    </button>
                </div>
            </center>
        </footer>
    </div>