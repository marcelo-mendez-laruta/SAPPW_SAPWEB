@{
    ViewData["Title"] = "Entrega/Eliminación de Tarjeta Paso1 - Consulta de Tarjetas en Trámite: ";
}
    <div class="container border border-primary" style="background-color:lightgrey">
        <header>
            <h4>Entrega/Eliminación de Tarjeta Paso1 - Consulta de Tarjetas en Trámite:</h4>
        </header>
        <form id="Tramite" name="tramite" method="post" action='@Url.Action("AlfabeticaTarjeta", "Consulta")'>
            <div class="container mb-2">
                <center>
                    <div class="row p-1">
                        <div class="col-sm-3">
                            <div class="form-group row p-1">
                                <label for="txtSucursal" class="col-sm-6 col-form-label">
                                    Sucursal:
                                </label>
                                <div class="col-sm-6">
                                    <input id="txtSucursal" class="form-control" type="text" autofocus />
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group row p-1">
                                <label for="txtAgencia" class="col-sm-6 col-form-label">
                                    Agencia:
                                </label>
                                <div class="col-sm-6">
                                    <input id="txtAgencia" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                    </div>
                </center>
            </div>
            <div class="container mb-2">
                <div class="form-row">
                    <div class="col-lg-11">
                        <div class="table-responsive bg-white" style="max-height:300px;">
                            <table class="table table-bordered table-bordered table-striped">
                                <thead class="thead-light">
                                    <tr>
                                        <th>No. Tarjeta</th>
                                        <th>Nombre del Titular</th>
                                        <th>Situación</th>
                                        <th>BCA..</th>
                                        <th>Otros</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <div class="col-lg-1">
                        <center>
                            <button type="button" class="w-100 btn btn-outline-secondary">
                                <i class="fas fa-print icono-2x"></i><br />Consultar
                            </button>
                            <button type="button" class="w-100 btn btn-outline-secondary">
                                <i class="fas fa-arrow-down icono-2x"></i><br />Siguiente
                            </button>
                            <button type="button" class="w-100 btn btn-outline-secondary">
                                <i class="fas fa-check icono-2x"></i><br />Seleccionar
                            </button>
                            <button type="button" class="w-100 btn btn-outline-secondary">
                                <i class="fas fa-window-close icono-2x"></i><br />Cerrar
                            </button>
                        </center>
                    </div>
                </div>
            </div>
            <div class="container p-1">
                <footer>
                    <center>
                        <div class="row">
                            <div class="col-sm-3">
                                <button type="button" class="btn border btn-light" onclick="cancelarProceso('@Url.Action("Index","Home")');">Cancelar</button>
                            </div>
                            <div class="col-sm-3">
                                <input class="btn-light btn-sm" type="submit" value=">>Siguiente>>" />
                            </div>
                        </div>
                    </center>
                </footer>
            </div>
        </form>
    </div>
