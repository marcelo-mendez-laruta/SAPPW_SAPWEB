@{

}
    <div class="container">
        <div class="row">
            <div class="col-sm-4" style="visibility:hidden;">
                <fieldset class="border border-dark ">
                    <legend class="w-auto" style="font-size:14px;">
                        Oficina
                    </legend>
                    <div class="container">
                        <div class="form-row">
                            <label for="txtAgencia" class="col-sm-6 col-form-label">
                                Sucursal:
                            </label>
                            <div class="col-sm-6">
                                <input id="txtSucursal" type="text" class="form-control" value="" />
                            </div>
                        </div>
                        <div class="form-row">
                            <label for="txtAgencia" class="col-sm-6">
                                Agencia:
                            </label>
                            <div class="col-sm-6">
                                <input id="txtAgencia"  type="text" class="form-control" value="204" />
                            </div>
                        </div>
                    </div>
                </fieldset>
                <fieldset class="border border-dark">
                    <legend class="w-auto" style="font-size:14px;">
                        Moneda
                    </legend>
                    <div class="container">
                        <div class="form-check">
                            <input id="rbMNacional" value="303" type="radio" class="form-check-input" checked />
                            <label for="rbMNacional" class="form-check-label">
                                Nacional
                            </label>
                        </div>
                        <div class="form-check">
                            <input id="rbMExtranjera" value="302" type="radio" class="form-check-input" />
                            <label for="rbMExtranjera" class="form-check-label">
                                Extranjera
                            </label>
                        </div>
                    </div>
                </fieldset>
				<fieldset class="border border-dark">
                    <legend class="w-auto" style="font-size:14px;">
                        Codigo CUI
                    </legend>
                    <div class="container">
                        <div class="form-row">
                            <label for="txtCui" class="col-sm-2 col-form-label">
                                CUI:
                            </label>
                            <div class="col-sm-10">
                                <input id="txtCui" name="cui" type="text" class="form-control" />
                            </div>
                        </div>
                    </div>
                </fieldset>
            </div>
            <div class="col-sm-8">
                <fieldset class="border border-dark p-2">
                    <legend class="w-auto" style="font-size:14px;">
                       Ingreso a Custodia DPF
                    </legend>
                    <div class="container">
                        <div class="form-row">
                            <label for="cbTC" class="col-sm-2 col-form-label">
                                Tipo Cuenta:
                            </label>
                            <div class="col-sm-10">
                                <select id="cbTC" name="indModalidad" class="form-control" required>
                                    <option  selected>Select</option>
                                    <option value="JU" >JURÍDICA</option>
                                    <option value="SL">ASOC. S/F LUCRO </option>
                                </select>
                            </div>
                        </div>
                        <div class="container mt-1">
                            <fieldset class="border border-dark">
                                <legend class="w-auto" style="font-size:13px;">
                                    Custodia DPF
                                </legend>
                                <div class="container">
                                    <div class="form-row">
                                        <label for="txtnDPF" class="col-sm-2 col-form-label">
                                            Nro DPF:
                                        </label>
                                        <div class="col-sm-10">
                                            <input id="txtnDPF" name="nroCuenta" type="text" class="form-control" required />
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </div>    
                    </div>
                </fieldset>
            </div>
        </div>
    </div>
